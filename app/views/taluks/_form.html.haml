= form_for @taluk do |f|
  - if @taluk.errors.any?
    #error_explanation
      %h2= "#{pluralize(@taluk.errors.count, "error")} prohibited this taluk from being saved:"
      %ul
        - @taluk.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :district
    = f.text_field :district
  .field
    = f.label :name
    = f.text_field :name
  .field
    = f.label :code
    = f.text_field :code
  .actions
    = f.submit 'Save'
