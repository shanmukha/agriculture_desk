= form_for @community_level do |f|
  - if @community_level.errors.any?
    #error_explanation
      %h2= "#{pluralize(@community_level.errors.count, "error")} prohibited this community_level from being saved:"
      %ul
        - @community_level.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :name
    = f.text_field :name
  .field
    = f.label :code
    = f.text_field :code
  .actions
    = f.submit 'Save'
