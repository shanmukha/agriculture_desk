= form_for [:super_admin,@community_type] do |f|
  - if @community_type.errors.any?
    #error_explanation
      %h2= "#{pluralize(@community_type.errors.count, "error")} prohibited this community_type from being saved:"
      %ul
        - @community_type.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :name
    = f.text_field :name
  .actions
    = f.submit 'Save'
